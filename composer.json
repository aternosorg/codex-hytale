{
  "name": "aternos/codex-hytale",
  "description": "PHP library to read, parse, print and analyse Hytale log files.",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Matthias",
      "email": "matthias@aternos.org"
    }
  ],
  "require": {
    "php": ">=8.4.0",
    "ext-json": "*",
    "aternos/codex": "^v4.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4"
  },
  "autoload": {
    "psr-4": {
      "Aternos\\Codex\\Hytale\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Aternos\\Codex\\Hytale\\Test\\": "test/tests/"
    }
  },
  "scripts": {
    "generate-tests": "php ./test/generate-tests.php",
    "test": "phpunit --testdox --config phpunit.xml"
  }
}
